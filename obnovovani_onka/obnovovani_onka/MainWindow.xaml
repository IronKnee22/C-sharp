<Window x:Class="obnovovani_onka.MainWindow"
        xmlns:local="clr-namespace:obnovovani_onka"
      
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
       
        mc:Ignorable="d"
        Title="Odpočet" Height="250" Width="400" WindowStartupLocation="CenterScreen" >

    <Grid>
        <TextBlock Name="Odpocet" Text="{Binding Zbyva}" FontSize="44" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
    </Grid>
</Window>
